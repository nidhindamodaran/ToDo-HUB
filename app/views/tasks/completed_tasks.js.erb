<% if params[:page].present?  %>
  <%# puts params %>
  $('#tasks_lists').append('<%= escape_javascript (render @tasks) %>');
  <% if @tasks.next_page %>
    $('.pagination').replaceWith('<%= escape_javascript will_paginate(@tasks) %>');
  <%else %>
    $('.pagination').remove();
  <%end%>
<% else %>
  <%# puts params %>
  $("#show_tabs").html("<%= escape_javascript(render partial: 'completed_tasks') %>");
<% end %>
