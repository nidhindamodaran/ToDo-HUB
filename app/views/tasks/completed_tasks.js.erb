<% if params[:page].present?  %>
  <%# puts params %>
  $('#tasks_lists').append('<%= j render(@tasks) %>');
  <% if @tasks.next_page %>
    $('.pagination').replaceWith('<%= j will_paginate(@tasks) %>');
  <%else %>
    $('.pagination').remove();
  <%end%>
<% else %>
  <%# puts params %>
  $("#show_tabs").html("<%= j(render(:partial => 'completed_tasks')) %>");
<% end %>
